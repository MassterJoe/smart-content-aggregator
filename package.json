{
  "name": "smart-content-aggregator",
  "version": "1.0.0",
  "description": "This a project for Tobams group associate backend role",
  "main": "package-build.js",
  "scripts": {
    "email-worker": "ts-node src/api/workers/emailWorker.ts",
    "tsoa:routes": "tsoa routes",
    "tsoa:spec": "tsoa spec",
    "preswagger:clean": "rimraf src/api/swagger",
    "tsoa:update": "npm run preswagger:clean && npm run tsoa:routes && npm run tsoa:spec",
    "dev": "npm run tsoa:update && nodemon --exec ts-node src/app.ts && npm run email-worker",
    "build": "rimraf dist && npm run tsoa:update && tsc",
    "lint": "eslint -c ./.eslintrc.js ./src --format stylish --fix --ext .ts",
    "start": "node dist/src/app.js",
    "migration:init": "migrate-mongo init",
    "migration:create": "migrate-mongo create",
    "migration:up": "migrate-mongo up",
    "migration:down": "migrate-mongo down",
    "migration:status": "migrate-mongo status"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MassterJoe/smart-content-aggregator.git"
  },
  "keywords": [],
  "author": "Salawu Olanrewaju J",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/MassterJoe/smart-content-aggregator/issues"
  },
  "homepage": "https://github.com/MassterJoe/smart-content-aggregator#readme",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@sendgrid/mail": "^8.1.5",
    "amqplib": "^0.10.8",
    "bcrypt": "^6.0.0",
    "chance": "^1.1.13",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "migrate-mongo": "^11.0.0",
    "moment": "^2.30.1",
    "mongoose": "^8.0.0",
    "natural": "^8.1.0",
    "nodemailer": "^7.0.5",
    "nodemon": "^3.1.10",
    "redis": "^5.8.1",
    "rimraf": "^6.0.1",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^6.6.0",
    "typedi": "^0.10.0",
    "uuidv4": "^6.2.13",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.7",
    "@types/bcrypt": "^6.0.0",
    "@types/chance": "^1.1.7",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.1",
    "@types/swagger-ui-express": "^4.1.8",
    "monnify-nodejs-lib": "file:third-party-libraries/Monnify-Nodejs-lib"
  }
}
